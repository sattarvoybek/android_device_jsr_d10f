# Camera
/data/cam_socket([0-9])+                      u:object_r:camera_socket:s0

# CMHW
/sys/devices/platform/kcal_ctrl.0(/.*)?       u:object_r:kcal_sysfs:s0
/sys/class/timed_output/vibrator/vtg_level    u:object_r:vibeamp_sysfs:s0

# MP-Decision
/data/system/default_values                   u:object_r:mpctl_data_file:s0
/dev/socket/mpctl                             u:object_r:mpctl_socket:s0

# Sensors
/persist/misc/sensors(/.*)?                   u:object_r:sensors_persist_file:s0

# Thermal
/system/bin/thermal-engine                    u:object_r:thermal-engine_exec:s0

# KCAL UI PostProcessing daemon
/system/bin/kcal-pp-daemon                    u:object_r:kcal-pp-daemon_exec:s0

# Torch
/sys/class/leds/torch-light(/.*)?             u:object_r:torch_sysfs:s0
/sys/devices/qcom,camera-led-flash.([0-9])+/leds/torch-light(/.*)? u:object_r:torch_sysfs:s0
