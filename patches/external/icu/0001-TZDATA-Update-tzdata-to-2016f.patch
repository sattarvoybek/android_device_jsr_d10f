+static s32 gtp_verify_main_clk(u8 *p_main_clk)
+{
+    u8 chksum = 0;
+    u8 main_clock = p_main_clk[0];
+    s32 i = 0;
+    
+    if (main_clock < 50 || main_clock > 120)    
+    {
+        return FAIL;
+    }
+    
+    for (i = 0; i < 5; ++i)
+    {
+        if (main_clock != p_main_clk[i])
+        {
+            return FAIL;
+        }
+        chksum += p_main_clk[i];
+    }
+    chksum += p_main_clk[5];
+    if ( (chksum) == 0)
+    {
+        return SUCCESS;
+    }
+    else
+    {
+        return FAIL;
+    }
+}
