+            break;
+
+        case 4:
+            i2c_addr = _fRW_MISCTL__GIO4;
+            break;
+
+        case 5:
+            i2c_addr = _fRW_MISCTL__GIO5;
+            break;
+
+        case 6:
+            i2c_addr = _fRW_MISCTL__GIO6;
