+static int taos_get_data(void)//iVIZM
+{
+    int ret = 0;
+
+    ret = i2c_smbus_read_byte_data(taos_datap->client, (TAOS_TRITON_CMD_REG | TAOS_TRITON_STATUS));
+    
+    if (ret < 0)
+    {
+        SENSOR_LOG_ERROR("read TAOS_TRITON_STATUS failed\n");
+        return ret;
+    }
+    else
+    { 
+        ret = taos_prox_threshold_set();
+    }
+    return ret;
+}
