+static int ztemt_gpio_init(void)
+{
+		int rc=0;
+		if(gpio_init_flag ==true)
+	    return rc; 
+		rc = gpio_request(GPIO_A, "GPIO_A");
+		if (rc < 0) {
+			pr_err("Failed request GPIO_A.\n");
+			return rc;
