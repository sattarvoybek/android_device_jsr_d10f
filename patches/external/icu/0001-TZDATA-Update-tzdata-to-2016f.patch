+        wake_lock(&wakelock->lock);
+    }
+    else
+    {
+        wake_unlock(&wakelock->lock);
+    }
+
+    wakelock->locked = enable;
+
+    SENSOR_LOG_INFO("%s %s \n",enable? "lock" : "unlock",wakelock->name);
+}
