+#define LOG_TAG "TEMP_HUMI"
+#define DEBUG_ON //DEBUG SWITCH
+
+#define SENSOR_LOG_FILE__ strrchr(__FILE__, '/') ? (strrchr(__FILE__, '/')+1) : __FILE__
+
+#ifdef  CONFIG_FEATURE_ZTEMT_SENSORS_LOG_ON
