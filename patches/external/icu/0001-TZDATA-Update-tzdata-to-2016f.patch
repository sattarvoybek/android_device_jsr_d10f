+    else if (ts->pnl_init_error)
+    {
+        GTP_INFO("Error occured in init_panel, no config sent");
+        return 0;
+    }
+    
+    GTP_INFO("Driver send config.");
+    for (retry = 0; retry < 5; retry++)
+    {
+        ret = gtp_i2c_write(client, config , GTP_CONFIG_MAX_LENGTH + GTP_ADDR_LENGTH);
+        if (ret > 0)
+        {
+            break;
+        }
+    }
+#endif
+    return ret;
+}
