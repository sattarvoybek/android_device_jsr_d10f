+static int tmg399x_resume(struct device *dev)
+{
+	struct tmg399x_chip *chip = dev_get_drvdata(dev);
+	int ret = 0;
+	mutex_lock(&chip->lock);
+    SENSOR_LOG_INFO("enter\n");
+	if(true == chip->prx_enabled)
+    {
+        SENSOR_LOG_INFO( "disable irq wakeup\n");
