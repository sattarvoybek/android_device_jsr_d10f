+    #else
+#define SENSOR_LOG_INFO(fmt, args...)
+#define SENSOR_LOG_DEBUG(fmt, args...)
+    #endif
+
+#else
+#define SENSOR_LOG_ERROR(fmt, args...)
+#define SENSOR_LOG_INFO(fmt, args...)
+#define SENSOR_LOG_DEBUG(fmt, args...)
