+	}
+    if(ret < 0)
+    {
+		SENSOR_LOG_INFO("disable_irq_wake failed\n");
+    }
+    SENSOR_LOG_INFO("eixt\n");
+	mutex_unlock(&chip->lock);
+    return ret ;
+}
