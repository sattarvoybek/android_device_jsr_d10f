+void ztemt_get_hw_version(char* result)
+{
+    int hw_id;
+    if(!result)
+		return;
+
+    hw_id = ztemt_get_hw_id();
+	
+    if(hw_id != HW_UN){
+        strcpy(result,hardware_id_map[hw_id].hw_ver); 
+     }else
+	    sprintf(result, "%s","unknow");
