+	int ret;
+	int reg;
+	int shift;
+
+	if( out <= AW_OUT_3 ){
+		reg = aw9106b_regs.led_gpio;
+		shift = out;
+	}
+	else{
+		reg = 0x12;
+		shift = out - AW_OUT_4;
+	}
+
+	if(gpmd_mode == AW_LED_MODE)
+		set = REG_BIT_CLEAR;
+	else
+		set = REG_BIT_SET;
+
+	ret = aw9106b_modify_regs(reg,shift,set);
